{
    "name": "await-result",
    "version": "2.2.1",
    "description": "Error handling for async functions without try/catch blocks",
    "author": {
        "name": "Ty-Lucas Kelley",
        "email": "ty@greylocklabs.com",
        "url": "https://greylocklabs.com"
    },
    "main": "dist/index.js",
    "keywords": [
        "async",
        "await",
        "async/await",
        "promises",
        "promise",
        "try/catch",
        "errors",
        "node"
    ],
    "homepage": "https://github.com/greylocklabs/await-result",
    "repository": {
        "type": "git",
        "url": "https://github.com/greylocklabs/await-result"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/greylocklabs/await-result/issues"
    },
    "scripts": {
        "reinstall": "rimraf node_modules && npm install",
        "clean": "rimraf dist",
        "prebuild": "npm run clean",
        "prepublishOnly": "npm run build && npm run test",
        "build": "babel --no-comments --minified src -d dist",
        "flow": "flow",
        "lint": "eslint src/**/*.js test/**/*.js",
        "pretest": "npm run build && npm run lint && npm run flow",
        "test": "cross-env NODE_ENV=test nyc ava test",
        "coveralls": "nyc report --reporter=text-lcov | coveralls",
        "precommit": "npm run lint && npm run flow",
        "prepush": "npm run test"
    },
    "engines": {
        "node": ">= 7.6"
    },
    "ava": {
        "require": "babel-register",
        "babel": "inherit",
        "failFast": true
    },
    "devDependencies": {
        "@greylocklabs/eslint-config": "1.12.2",
        "ava": "0.25.0",
        "babel-cli": "6.26.0",
        "babel-eslint": "8.2.5",
        "babel-plugin-add-module-exports": "0.2.1",
        "babel-plugin-istanbul": "4.1.6",
        "babel-preset-env": "1.7.0",
        "babel-preset-flow": "6.23.0",
        "coveralls": "3.0.2",
        "cross-env": "5.2.0",
        "eslint": "5.1.0",
        "eslint-plugin-ava": "4.5.1",
        "eslint-plugin-flowtype": "2.50.0",
        "eslint-plugin-import": "2.13.0",
        "eslint-plugin-promise": "3.8.0",
        "eslint-plugin-security": "1.4.0",
        "eslint-plugin-vue": "4.5.0",
        "flow-bin": "0.76.0",
        "husky": "0.14.3",
        "markdownlint-cli": "0.10.0",
        "nyc": "12.0.2",
        "rimraf": "2.6.2",
        "vue-eslint-parser": "3.1.0"
    }
}
