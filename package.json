{
    "name": "await-result",
    "version": "2.0.0",
    "description": "Error handling for async functions without try/catch blocks",
    "author": {
        "name": "Ty-Lucas Kelley",
        "email": "ty@greylocklabs.com",
        "url": "https://greylocklabs.com"
    },
    "main": "dist/index.js",
    "keywords": [
        "async",
        "await",
        "async/await",
        "promises",
        "promise",
        "try/catch",
        "errors",
        "node"
    ],
    "homepage": "https://github.com/greylocklabs/await-result",
    "repository": {
        "type": "git",
        "url": "https://github.com/greylocklabs/await-result"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/greylocklabs/await-result/issues"
    },
    "scripts": {
        "reinstall": "rimraf node_modules && npm install",
        "clean": "rimraf dist",
        "prebuild": "npm run clean",
        "prepublishOnly": "npm run build",
        "build": "babel --no-comments --minified src -d dist",
        "lint": "eslint --cache src/index.js test/index.js",
        "pretest": "npm run build",
        "test": "nyc ava test/index.js",
        "precommit": "npm run lint && npm run test",
        "prepush": "bash bin/prepush.bash"
    },
    "engines": {
        "node": ">= 7.6"
    },
    "ava": {
        "require": "babel-register",
        "babel": "inherit",
        "failFast": true
    },
    "devDependencies": {
        "@greylocklabs/eslint-config": "1.2.0",
        "ava": "0.23.0",
        "babel-cli": "6.26.0",
        "babel-plugin-add-module-exports": "0.2.1",
        "babel-preset-env": "1.6.1",
        "eslint": "4.11.0",
        "eslint-plugin-ava": "4.2.2",
        "eslint-plugin-import": "2.8.0",
        "eslint-plugin-promise": "3.6.0",
        "eslint-plugin-security": "1.4.0",
        "husky": "0.14.3",
        "nyc": "11.3.0",
        "rimraf": "2.6.2"
    }
}
